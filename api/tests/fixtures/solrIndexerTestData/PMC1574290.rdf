<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC1574290/reference/T2">
	<bibo:citedBy rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC1574290/section/Ever-never-HRT-use-by-index-date/paragraph_1"/>
