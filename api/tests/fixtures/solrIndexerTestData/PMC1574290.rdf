</rdf:Description>

<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC1574290/section/Methods/Objectives">
	<dcterms:hasPart rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC1574290/section/Objectives/paragraph_3"/>
