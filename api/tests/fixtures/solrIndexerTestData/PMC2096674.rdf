	<dcterms:hasPart rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096674/section/Life-Cycle/Older-Women"/>
</rdf:Description>

<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096674/section/Life-Cycle/Older-Women">
