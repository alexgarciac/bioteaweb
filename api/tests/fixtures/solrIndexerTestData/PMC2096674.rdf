	<dcterms:hasPart rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096674/section/Life-Cycle/Young-Women"/>
</rdf:Description>

<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096674/section/Life-Cycle/Young-Women">
	<dcterms:isPartOf rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096674"/>
	<dcterms:title>Young Women</dcterms:title>
</rdf:Description>

<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096674/section/Life-Cycle">
