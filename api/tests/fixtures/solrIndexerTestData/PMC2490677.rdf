	<dcterms:hasPart rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2490677/section/HIV-AIDS-related-knowledge/paragraph_4"/>
</rdf:Description>

<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2490677/section/HIV-AIDS-related-knowledge/paragraph_4">
	<dcterms:isPartOf rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2490677/section/Discussion/HIV-AIDS-related-knowledge"/>
	<rdf:type rdf:resource="http://www.w3.org/2011/content#Content"/>
	<rdf:type rdf:resource="http://www.w3.org/2011/content#ContentAsText"/>
	<cnt:chars>It is important to note that among the school course elements that have generated most controversy and debate in India are discussions about homosexuality and information on the options of safer sex, including condom use and masturbation.</cnt:chars>
	<cnt:characterEncoding>ISO-8859-1</cnt:characterEncoding>
</rdf:Description>

