<rdf:Description rdf:about="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096686/reference/B11">
	<bibo:citedBy rdf:resource="http://biotea.idiginfo.org/pubmedOpenAccess/rdf/PMC2096686/section/Societal-Factors-Associated-with-Chronic-Pain/paragraph_1"/>
